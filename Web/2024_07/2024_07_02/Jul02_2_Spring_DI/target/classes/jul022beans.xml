<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="d" class="com.kim.jul022.dog.Dog"></bean>
	
	<bean id="d2" class="com.kim.jul022.dog.Dog">
		<property name="name" value="개님"></property>
		<property name="age" value="99"></property>
	</bean>
	<bean id="d3" class="com.kim.jul022.dog.Dog">
		<constructor-arg value="개개"></constructor-arg>
		<constructor-arg value="13"></constructor-arg>
	</bean>
	<bean id="b1" class="com.kim.jul022.book.Book">
		<property name="title" value="태양의탑"></property>
		<property name="price" value="12000"></property>
	</bean>
	<bean id="b2" class="com.kim.jul022.book.Book">
		<constructor-arg value="눈마새"></constructor-arg>
		<constructor-arg value="9800"></constructor-arg>
	</bean>
</beans>
